<img src={{message.thumbnail_url}} height="32" with="32" local-class="avatar">

<div local-class="main">
  <div local-class="meta">
    <span local-class="username">{{message.author}}</span>
    <span local-class="timestamp">{{moment-format unixTimestamp 'h:mmA'}}</span>
  </div>

  <div local-class="text">{{markdown-it message.text inline=true}}</div>
</div>
